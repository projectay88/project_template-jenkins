#!groovy
// Check ub1 properties
properties([disableConcurrentBuilds()])

pipeline {
    agent {
            label any
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
    }
    stages {
        stage("First step") {
            steps {
                sh 'ssh deb@worker \'hostname\''
            }
        }
        stage("Second step") {
            steps {
                sh 'ssh deb@worker \'uptime\''
            }
        }
    }
}
